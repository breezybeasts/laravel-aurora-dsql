{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "breezybeasts/laravel-aurora-dsql",
    "description": "Laravel package for AWS Amazon Aurora DSQL",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike McCall",
            "email": "mike@breezybeasts.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/cache": "^10|^11",
        "aws/aws-sdk-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "BreezyBeasts\\AuroraDsql\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BreezyBeasts\\AuroraDsql\\Providers\\AuroraDsqlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
